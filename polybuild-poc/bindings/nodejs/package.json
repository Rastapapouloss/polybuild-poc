{
  "name": "node-polybuild",
  "version": "1.0.0",
  "description": "Node.js bindings for PolyBuild",
  "main": "index.js",
  "polybuild": {
    "linker": "nlink",
    "linkerMinVersion": "1.3.0",
    "priorityResolution": true,
    "compliance": "OBINexus-v1.0"
  },
  "binary": {
    "napi_versions": [8]
  },
  "dependencies": {
    "nlink-bindings": "^1.2.0"
  },
  "devDependencies": {
    "node-gyp": "^9.0.0"
  },
  "scripts": {
    "build": "node-gyp build",
    "test": "node test.js"
  },
  "keywords": ["polybuild", "nlink", "build-orchestration"],
  "license": "MIT"
}
